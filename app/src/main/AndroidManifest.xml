<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mnnitcentral">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".OfflineHandler"
        android:allowBackup="true"
        android:icon="@drawable/mni"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Create_Account"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Login_Page"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MoreInfo" />
        <activity android:name=".Welcome" />
        <activity android:name=".Change_Profile" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".MapsActivity" />
        <activity
            android:name=".Sub_Events_1"
            android:parentActivityName=".Events" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".List_of_Places"
            android:parentActivityName=".Welcome" />
        <activity android:name=".Details" />
        <activity
            android:name=".Events"
            android:parentActivityName=".Welcome" />
        <activity
            android:name=".Description"
            android:parentActivityName=".Welcome" />
        <activity android:name=".Settings" />
        <activity android:name=".Forget_Password" />
        <activity
            android:name=".Password_update"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".TLB" />
        <activity android:name=".Registration" />
        <activity android:name=".TimeTable" />
        <activity android:name=".Registration_Steps" />
        <activity android:name=".Show_Time_Table" />
        <activity android:name=".Scholarship" />
        <activity android:name=".Computer_Club" />
        <activity android:name=".Try"></activity>
    </application>

</manifest>